{
    "name": "nico-next",
    "version": "1.0.0",
    "description": "Main bot for Discord Clique",
    "main": "app.ts",
    "scripts": {
        "dev": "ts-node-dev --transpile-only --ignore-watch node_modules -r tsconfig-paths/register app.ts",
        "devtest": "ts-node-dev --transpile-only --ignore-watch node_modules -r tsconfig-paths/register test.ts",
        "dev:debug": "set DEBUG=consola && yarn dev",
        "build": "tsc",
        "start": "node -r ts-node/register/transpile-only -r tsconfig-paths/register dist/app.js",
        "update": "npm-check -u",
        "deploy": "bash ./deploy.sh",
        "typeorm": "ts-node --transpile-only -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
        "dyno:restart": "heroku scale worker=1 && heroku ps:restart worker.1 --app nico-discord-clique",
        "dyno:stop": "heroku scale worker=0",
        "prepare": "husky install",
        "prisma:update": "prisma migrate dev"
    },
    "engines": {
        "node": "16.x"
    },
    "author": "",
    "license": "ISC",
    "dependencies": {
        "@prisma/client": "^2.28.0",
        "@types/diff": "^5.0.0",
        "@types/express": "^4.17.11",
        "@types/fuzzy-search": "^2.1.0",
        "@types/mime-types": "^2.1.0",
        "@types/node-fetch": "^2.5.10",
        "@types/pako": "^1.0.1",
        "@types/ramda": "^0.27.41",
        "@types/require-all": "^3.0.2",
        "@types/spotify-web-api-node": "^5.0.3",
        "@types/twit": "^2.2.29",
        "@types/ws": "^7.4.4",
        "@types/youtube-dl": "^3.0.0",
        "@typescript-eslint/eslint-plugin": "^4.8.1",
        "@typescript-eslint/parser": "^4.8.1",
        "async-mutex": "^0.3.1",
        "bigint-conversion": "^2.1.12",
        "canvas": "^2.8.0",
        "chalk": "^4.1.1",
        "chrono-node": "^2.2.6",
        "consola": "^2.15.3",
        "date-fns": "^2.22.1",
        "dayjs": "^1.10.5",
        "diff": "^5.0.0",
        "discord.js": "^13.0.0-dev.4886ae2.1627257843",
        "emoji-regex": "^7.0.3",
        "esbuild": "^0.12.17",
        "eslint": "^7.26.0",
        "fastify": "^3.15.1",
        "file-type": "^16.5.0",
        "fuse.js": "^6.4.6",
        "fuzzy-search": "^3.2.1",
        "instagram-private-api": "^1.45.1",
        "leven": "^3.1.0",
        "mime-types": "^2.1.31",
        "node-fetch": "^2.6.1",
        "normalize-url": "^4.5.0",
        "npm-check": "^5.9.2",
        "ordinal": "^1.0.3",
        "pageres": "^6.2.3",
        "pako": "^2.0.3",
        "parse-duration": "^0.4.4",
        "patch-package": "^6.4.7",
        "prettier": "^2.3.2",
        "prisma": "^2.28.0",
        "radix-64": "^1.0.4",
        "ramda": "^0.27.1",
        "reflect-metadata": "^0.1.13",
        "remove": "^0.1.5",
        "require-all": "^3.0.0",
        "s-ago": "^2.2.0",
