{
    "compilerOptions": {
        "module": "commonjs",
        "moduleResolution": "node",
        "resolveJsonModule": true,
        "noImplicitAny": true,
        "removeComments": true,
        "target": "ESNext",
        "baseUrl": "./src",
        "outDir": "dist",
        "strict": true,
        "strictPropertyInitialization": false,
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
        "esModuleInterop": true,
        // "paths": {
        //     "src/*": [ "./src/*" ],
        // }
    }
}
